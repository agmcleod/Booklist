<h1>Listing all FAQ Categories</h1>

<ul>
<%= @faq_categories.each do |faq_category| %>
  <li>
    <%=h faq_category.name %> <%= link_to 'Show', faq_category %> 
    <%= link_to 'Edit', edit_faq_category_path(faq_category) %>
    <%= link_to 'Destroy', faq_category, :confirm => 'Are you sure?', :method => :delete %>
  </li>
<% end %>

<br />

<%= link_to 'New Category', new_faq_category_path %> <%= link_to "Back to support page", support_path %>